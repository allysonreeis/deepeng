{
    "version": 2,
    "name": "deepeng",
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.js"
        }
    ],
    "engines": {
        "node": "20.x"
    }
}